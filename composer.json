{
  "name": "tripsome/paybox",
  "description": "Acquiring system for accepting payments PayBox.money",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "dosarkz",
      "email": "ashenov.e@gmail.com"
    },
    {
      "name": "madyanmalfi",
      "email": "madyan@tripsome.com"
    }
  ],
  "keywords": ["Laravel", "Paybox"],
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Tripsome\\Paybox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tripsome\\Paybox\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tripsome\\Paybox\\PayboxServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
